<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="SearchDelayInMilliseconds" value="500" />
    <add key="TopResultCount" value="50" />
    <add key="JwtLifetimeInMinutes" value="30" />
    <add key="MinimumConfidenceScore" value="50" />

    <!-- Messaging extension AAD app -->
    <add key="MicrosoftAppId" value="" />

    <!-- SharePoint access AAD app -->
    <add key="GraphAppClientId" value="" />
    <add key="GraphAppClientSecret" value="" />

    <!-- Key securing the refresh endpoint -->
    <add key="RefreshEndpointKey" value="" />

    <!-- Configuration storage account -->
    <add key="StorageConnectionString" value="" />

    <!-- QnAMaker configuration-->
    <add key="QnAMakerHostUrl" value="" />
    <add key="QnAMakerSubscriptionKey" value="" />

    <!-- Encryption key for token -->
    <add key="TokenEncryptionKey" value="" />

    <add key="TenantId" value="" />
    <add key="AppBaseDomain" value="" />

    <add key="ApplicationInsightsInstrumentationKey" value="" />
    <add key="ApplicationInsightsLogLevel" value="Info" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6" />
    <customErrors mode="RemoteOnly" defaultRedirect="~/Error/Error" />
    <httpModules>
      <add name="TelemetryCorrelationHttpModule" type="Microsoft.AspNet.TelemetryCorrelation.TelemetryCorrelationHttpModule, Microsoft.AspNet.TelemetryCorrelation" />
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
    </httpModules>
  </system.web>
  <system.webServer>
    
    <modules>
      <remove name="TelemetryCorrelationHttpModule" />
      <add name="TelemetryCorrelationHttpModule" type="Microsoft.AspNet.TelemetryCorrelation.TelemetryCorrelationHttpModule, Microsoft.AspNet.TelemetryCorrelation" preCondition="managedHandler" />
      <remove name="ApplicationInsightsWebTracking" />
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" preCondition="managedHandler" />
    </modules>
    <httpProtocol>
      <customHeaders>
        <add name="Content-Security-Policy" value="frame-ancestors *.microsoft.com *.teams.microsoft.com *.skype.com;" />
        <add name="X-Content-Security-Policy" value="frame-ancestors *.microsoft.com *.teams.microsoft.com *.skype.com;" />
      </customHeaders>
    </httpProtocol>
    <validation validateIntegratedModeConfiguration="false" />
  <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers></system.webServer>
